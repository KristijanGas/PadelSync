<div class="HomePageHeader">
    <% if (!isAuthenticated) { %>
        <a href="/login">
            <button class="loginButton">Login</button>
        </a>
        <a href="/signup">
            <button class="registerButton">Register</button>
        </a>
    <% } else { %>
        <a href="/myprofile">
            <button class="myProfileButton">My Profile</button>
        </a>

        <a href="/myInbox">
            <button class="myProfileButton">Inbox</button>
        </a>

        <a href="/logout">
            <button class="logoutButton">Logout</button>
        </a>
    <% } %>
</div>